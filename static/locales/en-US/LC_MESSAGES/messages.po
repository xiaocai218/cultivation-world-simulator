# Action: Assassinate
msgid "assassinate_action_name"
msgstr "Assassinate"

msgid "assassinate_description"
msgstr "Assassinate the target; if failed, turns into combat"

msgid "assassinate_requirements"
msgstr "No restrictions; cooldown required"

